#include <windows.h>

IDI_ICON1		ICON		DISCARDABLE	"@CMAKE_SOURCE_DIR@/src/player/tano.ico"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @TANO_VERSION_MAJOR@,@TANO_VERSION_MINOR@,@TANO_VERSION_BUGFIX@,0
 PRODUCTVERSION @TANO_VERSION_MAJOR@,@TANO_VERSION_MINOR@,@TANO_VERSION_BUGFIX@,0
 FILEFLAGS 0x0L
 FILEFLAGSMASK 0x3fL
 FILEOS 0x00040004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName", "Tadej Novak"
            VALUE "FileDescription", "An open IP TV player"
            VALUE "FileVersion", "@TANO_VERSION@"
            VALUE "LegalCopyright", "Copyright (C) 2013 Tadej Novak."
            VALUE "InternalName", "Tano"
            VALUE "OriginalFilename", "Tano.exe"
            VALUE "ProductName", "Tano"
            VALUE "ProductVersion", "@TANO_VERSION@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0, 1200
    END
END
